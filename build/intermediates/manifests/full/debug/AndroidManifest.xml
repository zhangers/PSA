<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.FTD.UI"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="隼翼" >
        <activity
            android:name="com.FTD.UI.Start"
            android:theme="@style/NormalPageTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.FTD.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme" >
        </activity>
        <activity
            android:name="com.FTD.activity.First"
            android:label="@string/app_name"
            android:theme="@style/Theme" >
        </activity>
        <activity
            android:name="com.FTD.activity.Second"
            android:label="@string/app_name"
            android:theme="@style/Theme" >
        </activity>
        <activity
            android:name="com.FTD.activity.Third"
            android:label="@string/app_name"
            android:theme="@style/Theme" >
        </activity>
        <activity
            android:name="com.FTD.activity.Drawable"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.FTD.activity.Action"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.FTD.activity.AboutPage"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        </activity>

        <service android:name="com.FTD.service.FTDservice" >
        </service>
    </application>

</manifest>